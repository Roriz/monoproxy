app.{$ENRICHREADER_DOMAIN:enrichreader.localhost} {
	root * ../reader-app/build

	try_files {path} {path}/ /index.html

	encode zstd gzip

	file_server
}

api.{$ENRICHREADER_DOMAIN:enrichreader.localhost} {
	reverse_proxy 127.0.0.1:3000
}

app.{$ACTIVE_GENIE_DOMAIN:activegenie.localhost} {
	reverse_proxy 127.0.0.1:3001
}
